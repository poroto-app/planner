type Plan {
    id: String!
    name: String!
    places: [Place!]!
    timeInMinutes: Int!
    description: String
    transitions: [Transition!]!
    authorId: String
    likedPlaceIds: [String!]
}

type Transition {
    from: Place
    to: Place!
    duration: Int!
}

