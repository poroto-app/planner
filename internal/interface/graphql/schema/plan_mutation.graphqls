extend type Mutation {
    uploadPhotoToPlaceInPlan(inputs: [UploadPhotoToPlaceInPlanInput!]!): UploadPhotoToPlaceInPlanOutput!
}

input UploadPhotoToPlaceInPlanInput {
    # 画像投稿にはログインが必須
    userId: String!
    planId: String!
    placeId: String!
    urlOfPhoto: String!
}

type UploadPhotoToPlaceInPlanOutput {
    plan: Plan!
}