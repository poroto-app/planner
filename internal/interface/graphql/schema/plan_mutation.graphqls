extend type Mutation {
    uploadPlacePhotoInPlan(inputs: [uploadPlacePhotoInPlanInput!]!): uploadPlacePhotoInPlanOutput!
}

input uploadPlacePhotoInPlanInput {
    # 画像投稿にはログインが必須
    userId: String!
    planId: String!
    placeId: String!
    photoUrl: String!
}

type uploadPlacePhotoInPlanOutput {
    plan: Plan!
}