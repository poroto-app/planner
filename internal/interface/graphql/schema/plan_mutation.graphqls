extend type Mutation {
    uploadPlacePhotoInPlan(planId: String!, inputs: [UploadPlacePhotoInPlanInput!]!): UploadPlacePhotoInPlanOutput!
}

input UploadPlacePhotoInPlanInput {
    # 画像投稿にはログインが必須
    userId: String!
    placeId: String!
    photoUrl: String!
    width: Int!
    height: Int!
}

type UploadPlacePhotoInPlanOutput {
    plan: Plan!
}